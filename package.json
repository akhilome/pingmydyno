{
	"name": "pingmydyno",
	"version": "1.3.0",
	"description": "Keep Heroku dynos awake forever",
	"keywords": [
		"dyno",
		"heroku",
		"keep alive",
		"ping",
		"sleep",
		"wake"
	],
	"homepage": "https://github.com/codeshifu/pingmydyno#readme",
	"bugs": {
		"url": "https://github.com/codeshifu/pingmydyno/issues"
